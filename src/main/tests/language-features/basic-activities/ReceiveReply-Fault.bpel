<?xml version="1.0" encoding="UTF-8"?>
<process
        name="ReceiveReply-Fault"
        targetNamespace="http://dsg.wiai.uniba.de/bpel-engine-comparison/activities/bpel/receiveReplyFault"
        xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
        xmlns:ti="http://dsg.wiai.uniba.de/bpel-engine-comparison/activities/wsdl/testinterface"
        xmlns:bpel="http://docs.oasis-open.org/wsbpel/2.0/process/executable">
    <import namespace="http://dsg.wiai.uniba.de/bpel-engine-comparison/activities/wsdl/testinterface" location="../TestInterface.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <partnerLinks>
        <partnerLink name="MyRoleLink" partnerLinkType="ti:TestInterfacePartnerLinkType" myRole="testInterfaceRole"/>
    </partnerLinks>
    <variables>
        <variable name="FaultData" messageType="ti:executeProcessSyncFault"/>
        <variable name="InitData" messageType="ti:executeProcessSyncRequest"/>
    </variables>
    <sequence>
        <receive name="InitialReceive" createInstance="yes" partnerLink="MyRoleLink" operation="startProcessSync" portType="ti:TestInterfacePortType" variable="InitData"/>
        <assign name="AssignReplyData">
            <copy>
                <from variable="InitData" part="inputPart"/>
                <to variable="FaultData" part="payload"/>
            </copy>
        </assign>
        <reply name="ReplyToInitialReceive" partnerLink="MyRoleLink" operation="startProcessSync" portType="ti:TestInterfacePortType" faultName="ti:syncFault" variable="FaultData"/>
    </sequence>
</process>